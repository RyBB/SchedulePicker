{
  "name": "Schedule Picker",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "ハッカソン2017",
  "icons": {
    "16": "Artboard-n@2x.png",
    "48": "Artboard-n@2x.png",
    "128": "Artboard-n@2x.png"
  },
  "permissions": [
    "storage",
    "https://bozuman.cybozu.com/*",
    "tabs",
    "background",
    "contextMenus"
  ],
  "content_scripts": [{
    "matches": ["https://bozuman.cybozu.com/*"],
    "js": ["formatschedule.js", "makehtml.js", "script.js","jquery-3.2.1.min.js"]
  }],
  "background": {
    "matches": ["https://bozuman.cybozu.com/*"],
    "scripts": [
      "jquery-3.2.1.min.js",
      "background.js"
    ]
  },
  "browser_action": {
        "default_title": "action"
  }
}
